{
  "name": "sapper-starter",
  "description": "a sapper js site that can leverage markdown or pug template files with frontmatter on a netlify cms.",
  "version": "0.0.1",
  "license": "mine",
  "engines": {
    "node": ">=10.x"
  },
  "scripts": {
    "dev": "NODE_ENV=development run-p dev:*",
    "dev:sapper": "sapper dev",
    "dev:admin": "webpack --colors --watch --config static/admin/webpack.config.js",
    "dev:styles": "webpack --colors --watch --config static/styles/webpack.config.js",
    "build": " run-p build:*",
    "build:sapper": "sapper build",
    "build:admin": "webpack -p --config static/admin/webpack.config.js",
    "build:styles": "webpack -p --config static/styles/webpack.config.js",
    "export": " run-p build:admin build:styles && yarn run export:sapper",
    "export:sapper": "sapper export --entry \"/ /404 /discussions /board-meetings /about \"",
    "stage:export": "http-server __sapper__/export",
    "start": "yarn run dev",
    "start:server": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "npm-run-all --race dev cy:run"
  },
  "dependencies": {
    "ace-builds": "^1.4.7",
    "bootstrap": "^4.4.1",
    "clean-webpack-plugin": "^3.0.0",
    "compression": "^1.7.1",
    "css-loader": "^3.4.0",
    "front-matter": "^3.0.2",
    "glob": "^7.1.6",
    "http-server": "^0.12.1",
    "js-yaml": "^3.13.1",
    "markdown-it": "^10.0.0",
    "marked": "^0.8.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mz": "^2.7.0",
    "netlify-cms": "^2.10.9",
    "netlify-identity-widget": "^1.5.6",
    "node-sass": "^4.13.0",
    "polka": "next",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "pug": "^2.0.4",
    "purgecss-webpack-plugin": "^1.6.0",
    "react-ace": "^8.0.0",
    "sass-loader": "^8.0.0",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "sapper": "^0.27.0",
    "svelte": "^3.0.0",
    "svelte-loader": "^2.9.0",
    "svelte-scrollto": "^0.1.1",
    "webpack": "^4.7.0",
    "webpack-cli": "^3.3.10"
  }
}
